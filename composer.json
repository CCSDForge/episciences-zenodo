{
    "name": "episciences/zsubmit",
    "description": "An Episciences to Zenodo Proxy for handling submissions from Zenodo",
    "type": "project",
    "license": "GNU GENERAL PUBLIC LICENSE Version 3",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "guzzlehttp/guzzle": "^7.4",
        "knplabs/knp-paginator-bundle": "^5.8",
        "knpuniversity/oauth2-client-bundle": "^2.9",
        "l3/cas-guard-bundle": "^1.1",
        "league/oauth2-facebook": "^2.0",
        "michaelkaefer/oauth2-amazon": "^1.0",
        "monolog/monolog": "^2.3",
        "nyholm/psr7": "^1.4",
        "omines/oauth2-gitlab": "^3.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "psr/http-client": "^1.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/browser-kit": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^1.19.3",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/http-kernel": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "6.3.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/stopwatch": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bridge": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "6.3.*",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.7",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3"

    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*",
            "docker": true
        }
    }
}
